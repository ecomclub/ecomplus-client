{
  "name": "@ecomplus/client",
  "version": "2.0.2",
  "description": "JS client for E-Com Plus REST APIs",
  "main": "dist/ecom-client.node.js",
  "module": "src/index.js",
  "browser": "dist/ecom-client.min.js",
  "jsdelivr": "dist/ecom-client.polyfill.min.js",
  "unpkg": "dist/ecom-client.polyfill.min.js",
  "scripts": {
    "serve": "webpack-dev-server",
    "build": "node ./build/bin.js",
    "doc": "rm -rf ./docs/*; jsdoc ./src -r ./README.md -d ./docs -t ./node_modules/docdash",
    "release": "rm -rf ./dist; npm ddp && standard-version && node ./build/bin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecomclub/ecomplus-client.git"
  },
  "keywords": [
    "ecomplus",
    "ecommerce-apis",
    "js-client",
    "rest-apis",
    "jamstack",
    "headless-ecommerce"
  ],
  "author": "E-Com Club <ti@e-com.club>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecomclub/ecomplus-client/issues"
  },
  "homepage": "https://github.com/ecomclub/ecomplus-client#readme",
  "peerDependencies": {
    "@ecomplus/utils": "1.x"
  },
  "optionalDependencies": {
    "core-js": "3.x"
  },
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@ecomplus/utils": "^1.2.8",
    "babel-loader": "^8.0.6",
    "core-js": "^3.6.1",
    "docdash": "^1.1.1",
    "husky": "^3.1.0",
    "jsdoc": "^3.6.3",
    "standard": "^14.3.1",
    "standard-version": "^7.0.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "last 1 version",
    "not dead",
    "> 0.2%"
  ]
}
